@{
	Layout = "Shared/_Layout.cshtml";
}
	
@if (false)
{ 
	// this will not get compiled by the razor view engine, but gives us access to intellisense for the EUCLID js library
	<script type="text/javascript" src="../../Assets/Scripts/euclid-0.9.js"></script>
}

<script type="text/javascript">
	$(document).ready(function () {
		pageInit();
	});

	function pageInit() {
		GetData("/composite/api",
			function (data) {
				With(data).Fill("#composite").From("/composite/ui/template/composite");
				With(data).Fill("#commands").From("/composite/ui/template/commands");
				With(data).Fill("#queries").From("/composite/ui/template/queries");
				With(data).Fill("#input-models").From("/composite/ui/template/input-models");
				With(data).Fill("#agents").From("/composite/ui/template/agents");
			});
	}
</script>

<div class="row">
	<div class="span16" id="composite"></div>
</div>

<div class="row">
	<div class="span3" id="commands"></div>

	<div class="span3" id="queries"></div>

	<div class="span3" id="input-models"></div>

	<div class="span3" id="agents"></div>
</div>
