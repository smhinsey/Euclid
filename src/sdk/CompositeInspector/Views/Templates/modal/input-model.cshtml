<div>
	<div class="modal-header">
		<a href="#" class="close" data-dismiss="modal">×</a>
		<h3>{{ModelName}} <h5>for {{CommandName}}</h5></h3>
	</div>
	<div class="modal-body">
		{{#Properties}}
		{{#if Choices}}
			<h5>{{Name}} 
				<small>
					Select {{#if MultiChoice}}multiple{{else}}one{{/if}}
					[{{#comma-delimited-list Choices "" Value}}{{/comma-delimited-list}}]
				</small>
			</h5>
		{{else}}
			<h5>{{Name}} <small>{{Type}} {{#if Value}}({{Value}}){{/if}}</small></h5>
		{{/if}}
		{{/Properties}}
	</div>
	<div class="modal-footer">
		<a href="#" class="btn secondary" id="dismiss-modal">Close</a>
	</div>
</div>
