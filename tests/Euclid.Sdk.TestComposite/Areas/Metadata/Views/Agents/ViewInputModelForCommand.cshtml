@using AgentPanel.Areas.Metadata.Models
@using Euclid.Framework.Models
@model IInputModel

@using (Html.BeginForm("Publish", "Agents",  new { AgentSystemName = Model.AgentSystemName, PartName = Model.CommandName }, FormMethod.Post))
{
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>@Model.CommandName</legend>

        @Html.EditorFor(p => Model)
    </fieldset>

    <input type="submit" id="publish-command" value="Publish Command" />
}

@{
    var footer = (FooterLinkModel) ViewBag.Navigation;
    Html.RenderPartial("MetadataLinks", footer);
}
