@using AgentPanel.Areas.Metadata.Models
@model AgentListModel

<h1>@ViewBag.Tite</h1>

<ul>
  @foreach (var agent in this.Model.Agents)
  {
    <li>
      @agent.DescriptiveName - @agent.SystemName 
      (<a href="@Url.Action("Index", "Agents", new { AgentSystemName = agent.SystemName })">html</a> |
      <a href="@Url.Action("Index", "Agents", new { AgentSystemName = agent.SystemName, Format = "xml" })">xml</a> |
      <a href="@Url.Action("Index", "Agents", new { AgentSystemName = agent.SystemName, Format = "json" })">json</a>)
    </li>
  }
</ul>

@{ this.Html.RenderPartial("MetadataLinks", this.Model);  }