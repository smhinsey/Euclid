@model IAgentMetadata

<h1>@this.ViewBag.Title</h1>

<div class="property-set">
        
	@* 
        jt: not really sure why theo following doesn't work
        Html.LabelFor(x => x.Commands.Namespace)
        *@
	<div class="property-label">Command Namespace</div>
	<div class="property-value">
		<a href="@this.Url.Action("ListParts", "Agents", new { agentSystemName = this.Model.SystemName, partType = "Commands" })">
			@this.Model.Commands.Namespace
		</a>
	</div>
</div>
    
<div class="property-set">
	@*
        Html.LabelFor(x => x.Queries.Namespace)
        *@
	<div class="property-label">Query Namespace</div>
	<div class="property-value">
		<a href="@this.Url.Action("ListParts", "Agents", new { agentSystemName = this.Model.SystemName, partType = "Queries" })">
			@this.Model.Queries.Namespace
		</a>
	</div>
</div>

<a href="@this.Url.Action("Index")">Agents in Composite</a>