@using AdminComposite.Controllers
@using ForumAgent.ReadModels
@model ForumContent


@if (Model.ContentType == Enum.GetName(typeof(AvailableContentType), AvailableContentType.EmeddedYouTube))
{
	<iframe width="420" height="315" src="http://www.youtube.com/embed/@Model.Value" frameborder="0" allowfullscreen></iframe>
}
else
{
	@Html.Raw(Model.Value)
}