<script id="template-storeOrders" type="text/x-handlebars-template">
	<h3>{{storeName}} Orders</h3>
	<hr/>
	<table class="table table-striped">
		<thead>
			<tr>
				{{#each tableHeaders}}
				<th>{{this}}</th>
				{{/each}}
			</tr>
		</thead>
		<tbody>

			{{#each tableData}}
			<tr>
				<td>{{this.id}}...</td>
				<td>{{this.dateTime}}</td>
				<td>{{this.type}}</td>
				<td>{{this.amount}}</td>
				<td>
					<div class="btn-group">
						<a class="btn small" href="#">Action</a>
						<a class="btn small dropdown-toggle" data-toggle="dropdown" href="#">
							<span class="caret"></span>
						</a>
						<ul class="dropdown-menu">
							<li>
								<a>View Details</a>
							</li>
							<li>
								<a>Discount</a>
							</li>
							<li>
								<a>Modify</a>
							</li>
							<li>
								<a>Freeze</a>
							</li>
							<li>
								<a>Refund</a>
							</li>
						</ul>
					</div>
				</td>
			</tr>
			{{/each}}

		</tbody>
		<tfoot>
			<tr>
				<td colspan="6">
					<button class="btn pull-right">Add Manual Order</button>
				</td>
			</tr>
		</tfoot>
	</table>
</script>