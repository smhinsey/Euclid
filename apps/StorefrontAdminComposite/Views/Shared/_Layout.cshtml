<!DOCTYPE html>
<html>
	<head>
		<title>@ViewBag.Title</title>
		@Html.Partial("Styles")
		<link rel="shortcut icon" href="/Content/chromatron/img/favicon.ico">
	</head>
	<body>

		<div class="navbar navbar-static">

			<div class="navbar-inner">

				<div class="container">
					<a class="brand" href="#">@ViewBag.Title</a>
				</div>

			</div>

		</div>

		<div class="container">

			<div class="row">

				<div class="span3">

					@Html.Partial("LeftNav")

				</div>

				<div id="app-content" class="span9" style="background-color: ghostwhite;">
					@RenderBody()
				</div>

			</div>

		</div>
		@Html.Partial("Scripts")
	</body>
</html>